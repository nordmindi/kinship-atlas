{
  "name": "kinship-atlas",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "npm run supabase:start && npm run dev",
    "build": "tsc --noEmit && vite build",
    "build:deploy": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:run": "node scripts/test-run.mjs",
    "test:ci": "vitest run --coverage --reporter=verbose --reporter=json --outputFile=./test-results.json",
    "test:summary": "node scripts/test-summary.mjs",
    "type-check": "tsc --noEmit",
    "ci:check": "npm run lint && npm run type-check && npm run test:ci && npm run build",
    "supabase:start": "docker compose -f docker-compose.dev.yml up -d",
    "supabase:stop": "docker compose -f docker-compose.dev.yml down",
    "supabase:status": "docker compose -f docker-compose.dev.yml ps",
    "supabase:reset": "docker compose -f docker-compose.dev.yml down -v && docker compose -f docker-compose.dev.yml up -d",
    "supabase:local": "node scripts/switch-supabase.mjs local",
    "supabase:remote": "node scripts/switch-supabase.mjs remote",
    "backup:create": "node scripts/backup-database.mjs",
    "backup:list": "node scripts/list-backups.mjs",
    "health:check": "node scripts/health-check.mjs",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel"
  },
